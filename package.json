    "**/*.json": [
      "prettier --write"
    ],
    "**/*.md": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "release": {
    "repositoryUrl": "https://github.com/CraftFossLabs/react-beautiful-dragify.git",
    "branches": [
      "main",
      "next",
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  }
