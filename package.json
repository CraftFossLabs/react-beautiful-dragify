{
  "name": "react-beautiful-dragify",
  "version": "1.0.0-development",
  "description": "React drag and drop library alternatie of react-beautiful-dnd",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "lint": "eslint src/**/*.{ts,tsx}",
